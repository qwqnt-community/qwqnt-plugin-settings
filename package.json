{
  "$schema": "https://raw.githubusercontent.com/qwqnt-community/types/main/plugin.schema.json",
  "name": "qwqnt-plugin-settings",
  "version": "2.0.0",
  "description": "Support QwQNT plugins to add its settings page to settings of QQNT.",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "oxlint --fix --tsconfig ./tsconfig.json --type-aware src/**/*.js,src/**/*.ts",
    "build": "electron-vite build"
  },
  "author": "風間青祢",
  "license": "MIT",
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@qwqnt/types": "github:qwqnt-community/types",
    "@types/node": "^24.10.0",
    "electron": "^39.1.1",
    "electron-vite": "^4.0.1",
    "oxlint": "^1.26.0",
    "oxlint-tsgolint": "^0.5.1",
    "unplugin-oxlint": "^0.7.5",
    "unplugin-zip-pack": "^1.0.3",
    "vite": "^7.2.2",
    "vite-plugin-cp": "^6.0.3"
  },
  "packageManager": "pnpm@10.20.0",
  "qwqnt": {
    "name": "QwQNT 插件设置",
    "inject": {
      "main": "./main/index.js",
      "preload": "./preload/index.js",
      "renderer": "./renderer/index.js"
    },
    "dependencies": {
      "qwqnt-renderer-events": "^2.0.0"
    }
  }
}
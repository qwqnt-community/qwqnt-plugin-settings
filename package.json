{
  "$schema": "https://raw.githubusercontent.com/QwQ-002/types/main/plugin.schema.json",
  "name": "qwqnt-plugin_settings",
  "version": "1.2.5",
  "description": "Support QwQNT plugins to add its settings page to settings of QQNT.",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "oxlint --fix --tsconfig ./tsconfig.json --type-aware src/**/*.js,src/**/*.ts",
    "build": "electron-vite build"
  },
  "author": "Adpro",
  "license": "MIT",
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@qwqnt/types": "github:qwqnt-community/types",
    "@types/node": "^24.8.1",
    "electron": "^38.3.0",
    "electron-vite": "^4.0.1",
    "oxlint": "^1.23.0",
    "unplugin-oxlint": "^0.7.5",
    "unplugin-zip-pack": "^1.0.3",
    "vite": "^7.1.10",
    "vite-plugin-cp": "^6.0.3"
  },
  "packageManager": "pnpm@10.18.3",
  "qwqnt": {
    "name": "QwQNT 插件设置",
    "inject": {
      "main": "./main/index.js",
      "preload": "./preload/index.js",
      "renderer": "./renderer/index.js"
    },
    "dependencies": {
      "qwqnt-renderer_events": "^1.1.0"
    }
  }
}